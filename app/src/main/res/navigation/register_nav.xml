<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_nav"
    app:startDestination="@id/authentificationFragment">
    <fragment
        android:id="@+id/authentificationFragment"
        android:name="com.company.shakeup.AuthentificationFragment"
        android:label="fragment_authentification"
        tools:layout="@layout/fragment_authentification" >
        <action
            android:id="@+id/action_authentificationFragment_to_contactFragment"
            app:destination="@id/contactFragment" />
    </fragment>
    <fragment
        android:id="@+id/autorisationFragment"
        android:name="com.company.shakeup.AutorisationFragment"
        android:label="fragment_autorisation"
        tools:layout="@layout/fragment_autorisation" >
        <argument
            android:name="user_id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.company.shakeup.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" >
        <action
            android:id="@+id/action_contactFragment_to_autorisationFragment"
            app:destination="@id/autorisationFragment" />
        <argument
            android:name="id_user"
            app:argType="string" />
    </fragment>
</navigation>